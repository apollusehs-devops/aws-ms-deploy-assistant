<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="AWSDeploymentAssistant.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    </sectionGroup>
    <section name="aws" type="Amazon.AWSSection, AWSSDK"/>
  </configSections>
  <log4net>
    <appender name="LogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="%property{LogPath}\\%date{yyyy-MM-dd-HH-mm-ss}.log" />
      <staticLogFileName value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%d [%t] %-5p %c %m%n" />
      </layout>
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
    </appender>
    <appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender">
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%d [%t] %-5p %c %m%n" />
      </layout>
    </appender>
    <logger name="Default">
      <level value="ALL" />
      <appender-ref ref="ConsoleAppender" />
      <appender-ref ref="LogFileAppender" />
    </logger>
  </log4net>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <probing privatePath="bin;bin\plugins" />
    </assemblyBinding>
  </runtime>
  <applicationSettings>
    <AWSDeploymentAssistant.Properties.Settings>
      <setting name="DefaultLoggerName" serializeAs="String">
        <value>Default</value>
      </setting>
      <setting name="PublishFileTypesExcludePattern" serializeAs="Xml">
        <value>
          <ArrayOfString xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <string>obj\</string>
            <string>Web.Debug.config</string>
            <string>Web.Release.config</string>
            <string>packages.config</string>
          </ArrayOfString>
        </value>
      </setting>
      <setting name="PublishFileTypesIncludePattern" serializeAs="Xml">
        <value>
          <ArrayOfString xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <string>.dll</string>
            <string>.css</string>
            <string>.js</string>
            <string>.xml</string>
            <string>.xsl</string>
            <string>.xslt</string>
            <string>.xsd</string>
            <string>.htm</string>
            <string>.html</string>
            <string>.xhtml</string>
            <string>.cshtml</string>
            <string>.aspx</string>
            <string>.ascx</string>
            <string>.asax</string>
            <string>.asmx</string>
            <string>.axd</string>
            <string>.config</string>
            <string>.ico</string>
            <string>.jpg</string>
            <string>.gif</string>
            <string>.png</string>
            <string>.eot</string>
            <string>.svg</string>
            <string>.ttf</string>
            <string>.woff</string>
            <string>.woff2</string>
            <string>.otf</string>
            <string>.exe</string>
            <string>.json</string>
            <string>.swf</string>
            <string>.swz</string>
            <string>.ashx</string>
            <string>.asp</string>
            <string>.rdf</string>
            <string>.flv</string>
            <string>.svc</string>
            <string>.mime</string>
            <string>.rss</string>
            <string>.atom</string>
            <string>.yml</string>
            <string>.ps1</string>
            <string>.zip</string>
          </ArrayOfString>
        </value>
      </setting>
      <setting name="LogFolderPath" serializeAs="String">
        <value>C:\\temp\\AWS Development Tools\\EC2 Deployment Assistant\\logs</value>
      </setting>
      <setting name="TempFolderPath" serializeAs="String">
        <value>C:\\temp\\AWS Development Tools\\EC2 Deployment Assistant\\temp</value>
      </setting>
      <setting name="DefaultProfileName" serializeAs="String">
        <value>default</value>
      </setting>
    </AWSDeploymentAssistant.Properties.Settings>
  </applicationSettings>
</configuration>